ll Digits(ll a)
{
    return(floor(log10(a))+1);
}
ll BMod(ll B,ll P,ll M)
{
    ll R=1;
    B=B%M;
    while(P)
    {
        if(P&1)
            R=(R*B)%M;
        P >>= 1;
        B=(B*B)%M;
    }
    return R;
}
ll POW(ll  B,ll P)
{
    ll R=1;
    while(P)
    {
        if(P&1)
            R=R*B;
        B=B*B;
        P>>=1;
    }
    return R;
}
ll gcd(ll a, ll b)
{
    if (b == 0)
        return a;
    return gcd(b, a % b);

}
ll lcm(ll a,ll b)
{
    return a*b/gcd(a,b);

}


ll dp[100005];


ll stair(ll n)
{
    
    if(n==1)
        return 1;
    else if(n==2)
        return 2;
    else if(n==3)
        return 4;

    else
    {
        if(dp[n]!= -1)
            return dp[n];
        else
        {
            dp[n]= ( stair(n-1)+stair(n-2)+stair(n-3) )%mod;
            return dp[n];
        }
    }
}


